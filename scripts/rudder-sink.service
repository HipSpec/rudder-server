[Unit]
Description = Setting up Rudder Sink
After = network.target

[Service]
WorkingDirectory = /home/ubuntu/rudder-server/
Restart = on-failure
RestartSec = 3s
StartLimitInterval = 0
ExecStart = /home/ubuntu/rudder-server/tests/sink
StandardOutput = syslog
StandardError = syslog
SyslogIdentifier = sink


[Install]
WantedBy = multi-user.target
